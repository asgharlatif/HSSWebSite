<slide_show>
  <options>
    <debug>false</debug>
    <background>transparent</background>
    <!-- #RRGGBB, transparent -->
    <margins>
      <vertical_ratio>43%</vertical_ratio>
      <top>-40</top>
    </margins>
    <interaction>
      <speed>45</speed>
      <!-- [-360,360] degrees per second -->
      <default_speed>5</default_speed>
      <default_view_point>40%</default_view_point>
      <!-- [0,100] percentage -->
      <reset_delay>5</reset_delay>
      <!-- [0,600] seconds, 0 means never reset -->
      <rotation>mouse</rotation>
      <!-- auto, mouse, keyboard -->
      <view_point>none</view_point>
      <!-- none, mouse, keyboard -->
    </interaction>

    <far_photos>
      <size>10%</size>
      <!-- [0,100] percentage -->
      <amount>80%</amount>
      <!-- [0,100] percentage -->
      <blur>40</blur>
      <!-- [0,100] amount -->
      <blur_quality>3</blur_quality>
      <!-- [1,3] 1=low - 3=high -->
    </far_photos>

    <titles>
      <style>font-size: 12px; font-family: Verdana, _serif; color: #333333;</style>
      <position>above right</position>
      <!-- [above, below] [left,center,right]-->

      <scale9>10 10 10 10</scale9>
      <!-- [0,1000] pixels -->
      <padding>5 5 5 5</padding>
      <!-- [-1000,1000] pixels -->
    </titles>


  </options>

